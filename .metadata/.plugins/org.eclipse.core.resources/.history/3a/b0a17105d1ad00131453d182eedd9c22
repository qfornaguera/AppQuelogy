package com.example.appqueology;

import android.graphics.PointF;

import com.mindfusion.diagramming.DiagramAdapter;
import com.mindfusion.diagramming.DiagramNode;
import com.mindfusion.diagramming.NodeEvent;
import com.mindfusion.diagramming.ShapeNode;


public class AppqueologyDiagramAdapter extends DiagramAdapter{
	
	public AppqueologyDiagramAdapter(){
		
	}
	
	public void nodeCreated(NodeEvent e){
		DiagramNode  node = e.getNode();
		PointF pointer = e.getPointerPosition();
		ShapeNode shapeNode = (ShapeNode)node;
		shapeNode.setText(pointer.x+" "+pointer.y);
		
	}

}
