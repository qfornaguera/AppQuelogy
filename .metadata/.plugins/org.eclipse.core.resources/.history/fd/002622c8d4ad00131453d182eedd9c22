package com.example.appqueology;

import android.graphics.Point;
import android.graphics.PointF;
import android.view.MotionEvent;
import android.webkit.WebView.FindListener;

import com.mindfusion.diagramming.BehaviorBase;
import com.mindfusion.diagramming.Diagram;
import com.mindfusion.diagramming.DiagramView;
import com.mindfusion.diagramming.InteractionState;
import com.mindfusion.diagramming.ShapeNode;

public class AppqueologyBehaviorBase extends BehaviorBase{

	protected AppqueologyBehaviorBase(DiagramView flowChart) {
		super(flowChart);
		
	}

	@Override
	protected InteractionState startDraw(PointF arg0, MotionEvent arg1) {
		// TODO Auto-generated method stub
		
		return null;
	}
	
	
	@Override
	protected void pointerUp(Point pointerPosition,MotionEvent e){


		Diagram diagram = getDiagram();
		ShapeNode newNode = diagram.getFactory().createShapeNode(pointerPosition.x,pointerPosition.y,25,25);
        newNode.setId(Global.ID);
        Global.ID++;
	}
}
